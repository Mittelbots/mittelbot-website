{
  "name": "mittelblot-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "code:eslint": "eslint --ext .js,.html ./ --c .tools/eslint/.eslintrc.cjs --cache --cache-location .tools/eslint/.cache/",
    "code:lint": "npm run code:eslint && npm run scss:lint",
    "code:format": "npm run code:eslint -- --fix && npm run scss:lint -- --fix",
    "scss:lint": "stylelint src/assets/scss --config .tools/stylelint/stylelintrc.json",
    "start:dev": "docker compose up -d && npm run logs",
    "restart": "npm run stop && npm run start:dev",
    "logs": "docker compose logs -f",
    "stop": "docker compose down"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@html-eslint/parser": "^0.22.0",
    "@nuxt/devtools": "latest",
    "@nuxt/typescript-build": "^3.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "nuxt": "^3.8.2",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.1",
    "sass-loader": "^13.3.2",
    "stylelint": "^16.0.2",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "stylelint-scss": "^6.0.0",
    "typescript": "^5.3.3",
    "vue": "^3.3.12",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^3.0.2",
    "bootstrap": "^5.3.2",
    "sass": "^1.69.5"
  }
}
